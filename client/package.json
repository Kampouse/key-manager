{
  "name": "near-fastkv-encrypted",
  "version": "0.1.0",
  "description": "End-to-end encrypted key-value storage for NEAR Protocol using FastKV backend. Requires NEAR account and FastKV infrastructure. Client-side AES-256-GCM encryption with TEE key wrapping via OutLayer.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "encryption",
    "key-value",
    "e2ee",
    "tee",
    "storage",
    "near",
    "fastkv",
    "aes-256-gcm"
  ],
  "author": "Kampouse",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kampouse/key-manager.git",
    "directory": "client"
  },
  "bugs": {
    "url": "https://github.com/Kampouse/key-manager/issues"
  },
  "homepage": "https://github.com/Kampouse/key-manager#readme",
  "dependencies": {
    "near-api-js": "^7.1.1",
    "ofetch": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
